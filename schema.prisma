datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int      @id @default(autoincrement())
  email    String   @unique
  password String
}

model Banner {
  id        Int      @id @default(autoincrement())
  title     String   @unique
  imageData String
  position  String
  active    Boolean
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}